      "source": [
        "def mul_m(A, B):\r\n",
        "    C = [[0 for row in range(len(B[0]))] for col in range(len(A))]\r\n",
        "    for i in range(len(A)):\r\n",
        "        for j in range(len(B[0])):\r\n",
        "            for k in range(len(B)):\r\n",
        "                C[i][j] += A[i][k]*B[k][j]\r\n",
        "    r = gg.matrix(C)\r\n",
        "    return r"
      ],
      "execution_count": 14,
      "outputs": []
    },
    {

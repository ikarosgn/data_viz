      "cell_type": "code",
      "metadata": {
        "id": "_ccMXgmvhVtd"
      },
      "source": [
        "def mat_mul(A,B):\r\n",
        "    C = np.zeros((A.shape[0],B.shape[1]))\r\n",
        "    for r_a in range(A.shape[0]):\r\n",
        "        for c_b in range(B.shape[1]):\r\n",
        "            for every_element in range(A.shape[1]):\r\n",
        "              C[r_a,c_b] = C[r_a,c_b] + A[r_a,every_element]*B[every_element,c_b]\r\n",
        "    return C"
      ],
      "execution_count": 38,
      "outputs": []
    },
    {
